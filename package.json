{
    "name": "react-typescript-vite-test",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:mock": "cross-env-shell VITE_NODE_MODE=MOCK \"pnpm run mock & vite\"",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "mock": "node src/mock/db.tpl.js && sleep 1 && json-server -p 3001 src/mock/db.json",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.85.3",
        "@vanilla-extract/css": "^1.17.4",
        "axios": "^1.11.0",
        "immer": "^10.1.1",
        "lodash": "^4.17.21",
        "nuqs": "^2.4.3",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router": "^7.8.1",
        "zod": "^4.0.17",
        "zustand": "^5.0.7"
    },
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@faker-js/faker": "^9.9.0",
        "@redux-devtools/extension": "^3.3.0",
        "@tanstack/eslint-plugin-query": "^5.83.1",
        "@testing-library/jest-dom": "^6.7.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/lodash": "^4.17.20",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@vanilla-extract/vite-plugin": "^5.1.1",
        "@vitejs/plugin-react": "^5.0.1",
        "@vitejs/plugin-react-swc": "^4.0.0",
        "@vitest/coverage-v8": "^3.2.4",
        "cross-env": "^10.0.0",
        "eslint": "^9.33.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "jsdom": "^26.1.0",
        "json-server": "1.0.0-beta.3",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.39.1",
        "vite": "^7.1.2",
        "vitest": "^3.2.4"
    }
}
